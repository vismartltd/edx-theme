}

.user-panel {
	position: relative;
	width: 0;
	bottom: -60px;
	left: 25px;
	transform: translate(0%, 0%);
	overflow: hidden;
	z-index: 666;
	-webkit-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	-moz-transition: all 0.5s ease;
	transition: all 0.5s ease;
}

.user-panel ul {
	position: relative;
	top: 0;
	left: 0;
	width: 190px;
	box-sizing: border-box;
	padding: 0;
	margin: 0;
	list-style: none;
	font-family: Circe;
	font-size: 20px;
	border: 1px solid rgba(255, 255, 255, 0.8);
	border-radius: 10px;
	overflow: hidden;
}

.show-panel {
	width: 190px;
	transform: translate(-50%, 0%);
}

.user-panel a {
	display: block;
	width: 100%;
	padding: 8px 8px 8px 8px;
	text-decoration: none;
	box-sizing: border-box;
	background-color: rgba(255, 255, 255, 0.8);
	line-height: 30px;
	color: #000;
	-webkit-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	-moz-transition: all 0.5s ease;
	transition: all 0.5s ease;
}

.user-panel a:hover {
	background-color: rgba(150, 150, 150, 0.8);
}

.user-panel ul:before, .user-panel ul:after {
	content: " ";
	display: table;
}

.user-panel ul:after {
	clear: both;
}

