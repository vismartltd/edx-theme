.skin.test {
	height: 100%;
	width: 100%;
	position: absolute;
	background-color: rgba(0, 0, 0, 0.1);
}

.course-info {
	position: relative;
	display: block;
	min-height: 300px;
	width: calc(100% - 60px);
	width: -wedkit-calc(100% - 60px);
	width: -moz-calc(100% - 60px);
	margin: 0 0 30px 30px;
	border: 5px solid rgb(255, 119, 54);
	color: rgb(255, 119, 54);
	font-family: Circe;
	box-sizing: border-box;
}

.course-info-question-scale:before, .course-info-question-scale:after {
	content: " ";
	display: table;
}

.course-info-question-scale:after {
	clear: both;
}

.course-info-title {
	display: inline-block;
	float: left;
	width: calc(100% - 390px);
	width: -wedkit-calc(100% - 390px);
	width: -moz-calc(100% - 390px);
	font-size: 20px;
	line-height: 20px;
	margin: 30px 30px 10px 30px;
}

.course-info-desc {
	display: inline-block;
	float: left;
	width: calc(100% - 390px);
	width: -wedkit-calc(100% - 390px);
	width: -moz-calc(100% - 390px);
	color: #000;
	font-size: 14px;
	line-height: 20px;
	margin: 10px 30px 0 30px;
}

.course-info-time {
	position: absolute;
	top: 30px;
	right: -30px;
	height: 80px;
	width: 300px;
	line-height: 85px;
	font-size: 100px;
	text-align: left;
}

.course-info-question-scale {
	position: relative;
	display: inline-block;
	left: -5px;
	width: calc(100% - 50px);
	width: -wedkit-calc(100% - 50px);
	width: -moz-calc(100% - 50px);
	height: 50px;
	margin: 30px;
}

.course-info-question-scale div {
	display: inline-block;
	height: 100%;
	width: 8px;
	margin: 0 5px;
	background-color: #000;
	opacity: 1;
	-webkit-transition: opacity 1s ease;
    -o-transition: opacity 1s ease;
    -moz-transition: opacity 1s ease;
    transition: opacity 1s ease;
}

.column-1, .column-2 {
	position: relative;
	display: inline-block;
	float: left;
	width: calc(50% - 45px);
	width: -wedkit-calc(50% - 45px);
	width: -moz-calc(50% - 45px);
	font-family: Circe;
}

.column-1 {
	padding: 0 15px 30px 30px;
}

.column-2 {
	padding: 0 30px 30px 15px;
}

.column-1:before, .column-2:before, .column-1:after, .column-2:after {
	content: " ";
	display: table;
}

.column-1:after, .column-2:after {
	clear: both;
}

.question-title {
	display: inline-block;
	float: left;
	width: calc(100% - 50px);
	width: -wedkit-calc(100% - 50px);
	width: -moz-calc(100% - 50px);
}

.question-number {
	display: inline-block;
	float: left;
	width: 50px;
	text-align: right;
}

.question-content {
	display: inline-block;
	float: left;
	width: calc(100% - 60px);
	width: -wedkit-calc(100% - 60px);
	width: -moz-calc(100% - 60px);
	padding: 30px;
	margin-top: 30px;
	outline: 3px solid #888;
	min-height: 50px;
}

.variants {
	position: relative;
	display: inline-block;
	width: 100%;
	text-align: right;
}

.question {
	position: relative;
	display: inline-block;
	float: left;
	width: 100%;
	margin-top: 30px;
	outline: 3px solid #888;
	min-height: 50px;
	-webkit-transition: all 0.5s ease;
    -o-transition: all 0.5s ease;
    -moz-transition: all 0.5s ease;
    transition: all 0.5s ease;
}

.question.active, .question.active .question-variant {
	outline: 3px solid rgb(255, 119, 54);
}

.question:hover {
	cursor: pointer;
}

.question:before, .question:after {
	content: " ";
	display: table;
}

.question:after {
	clear: both;
}

.question-number-variant {
	position: relative;
	display: inline-block;
	float: left;
	width: 10px;
	padding: 30px;
	text-align: center;
}

.question-variant {
	position: relative;
	display: inline-block;
	float: left;
	width: calc(100% - 130px);
	width: -wedkit-calc(100% - 130px);
	width: -moz-calc(100% - 130px);
	padding: 30px;
	outline: 3px solid #888;
	text-align: center;
	-webkit-transition: all 0.5s ease;
    -o-transition: all 0.5s ease;
    -moz-transition: all 0.5s ease;
    transition: all 0.5s ease;
}

.button {
	display: inline-block;
	position: relative;
	height: auto;
	width: 100%;
	padding: 10px;
	margin-top: 30px;
	background-color: rgb(255, 119, 54);
	font-family: Circe-Bold;
	box-sizing: border-box;
	outline: 3px solid rgb(255, 119, 54);
	font-size: 30px;
	text-align: center;
	opacity: 1;
	-webkit-transition: all 0.5s ease;
    -o-transition: all 0.5s ease;
    -moz-transition: all 0.5s ease;
    transition: all 0.5s ease;
}

.button:hover {
	cursor: pointer;
	opacity: 1;
}

.disabled {
	background-color: #888;
	outline: 3px solid #888;
	opacity: 0.5;
}

.disabled:hover {
	cursor: default;
}
