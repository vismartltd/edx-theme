}

.left-arrow polyline {
	stroke: rgb(255, 255, 255);
	opacity: 0.3;
	-webkit-transition: opacity 0.6s ease;
    -o-transition: opacity 0.6s ease;
    -moz-transition: opacity 0.6s ease;
    transition: opacity 0.6s ease;
}

.left-arrow.dark polyline {
	stroke: rgb(0, 0, 0);
	opacity: 0.2;
}

.left-arrow:hover polyline {
