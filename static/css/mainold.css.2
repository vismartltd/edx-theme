#wrapper {
	position: relative;
}

.main-section {
	position: relative;
	width: 100%;
	height: 100vh;
	min-height: 100vh;
}

.slider {
	position: absolute;
	top: 0;
	left: 0;
	-webkit-transform: translate(0%, 0%);
	-ms-transform: translate(0%, 0%);
	transform: translate(0%, 0%);
	width: 100%;
	height: 100%;
    -webkit-transition: transform 0.6s ease;
    -o-transition: transform 0.6s ease;
    -moz-transition: transform 0.6s ease;
    transition: transform 0.6s ease;
}

.slide-first {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: #f2f2f2 center center no-repeat;
    background-size: cover;
	background-image: url('/static/themes/ninenone/images/drivers.jpg');
}

.slide-second {
	position: absolute;
	top: 0;
	left: 100%;
	width: 100%;
	height: 100%;
	background: #f2f2f2 center center no-repeat;
    background-size: cover;
	background-image: url('/static/themes/ninenone/images/driver.jpg');
}

.slide-third {
	position: absolute;
	top: 0;
	left: 200%;
	width: 100%;
	height: 100%;
	background: #f2f2f2 center center no-repeat;
    background-size: cover;
	background-image: url('/static/themes/ninenone/images/tank.jpg');
}

.skin {
	height: inherit;
	width: inherit;
	position: absolute;
	background-color: rgba(0, 0, 0, 0.5);
}

.modal {
	position: fixed;
	display: table;
	top: 0;
	width: 100%;
	height: 100%;
	background-color: rgba(0, 0, 0, 0.8);
	opacity: 1;
	-webkit-transition: opacity 0.6s ease;
    -o-transition: opacity 0.6s ease;
    -moz-transition: opacity 0.6s ease;
    transition: opacity 0.6s ease;
}

.hidden {
	opacity: 0;
}

.modal-center {
	position: relative;
	display: table-cell;
	height: 50%;
	vertical-align: middle;
	text-align: center;
	font-family: Circe;
}

.modal-center-ask {
	display: inline-block;
	width: 100%;
	padding: 0 30px 0 30px;
	box-sizing: border-box;
	color: #fff;
	font-size: 25px;
}

.modal-basic-course, .modal-current-course {
	display: inline-block;
	position: relative;
	height: auto;
	width: 300px;
	padding: 10px;
	margin: 30px;
	background-color: rgba(255, 119, 54, 0);
	font-family: Circe-Bold;
	box-sizing: border-box;
	outline: 3px solid rgba(255, 119, 54, 1);
	color: rgba(255, 119, 54, 1);
	font-size: 30px;
	text-align: center;
	text-decoration: none;
	opacity: 1;
	-webkit-transition: all 0.6s ease;
    -o-transition: all 0.6s ease;
    -moz-transition: all 0.6s ease;
    transition: all 0.6s ease;
}

.modal-basic-course:hover, .modal-current-course:hover {
	background-color: rgba(255, 119, 54, 1);
	outline: 3px solid rgba(0, 0, 0, 0);
	color: #000;
	cursor: pointer;
}

.burger-close {
	position: absolute;
	display: block;
	top: 30px;
	right: 30px;
	height: 36px;
	width: 36px;
	-webkit-transform: rotate(45deg);
	-ms-transform: rotate(45deg);
	transform: rotate(45deg);
	-webkit-transition: all 0.2s ease;
	-o-transition: all 0.2s ease;
	-moz-transition: all 0.2s ease;
	transition: all 0.2s ease;
}

.burger-close span {
	position: absolute;
    display: block;
    width: 36px;
    height: 5px;
    left: 0;
    margin: 0 0 0 0;
    background: #fff;
    -webkit-transition: all 0.2s ease;
    -o-transition: all 0.2s ease;
    -moz-transition: all 0.2s ease;
    transition: all 0.2s ease;
}

.burger-close span:nth-child(1) {
	top: 15.5px;
	width: 100%;
	-webkit-transform: rotate(90deg);
	-ms-transform: rotate(90deg);
	transform: rotate(90deg);
}

.burger-close span:nth-child(2) {
    top: 15.5px;
    width: 100%;
}

.burger-close:hover {
    cursor: pointer;
    -webkit-transform: rotate(0deg);
    -ms-transform: rotate(0deg);
    transform: rotate(0deg);
}

.burger-close:hover span:nth-child(1) {
	-webkit-transform: rotate(180deg);
	-ms-transform: rotate(180deg);
	transform: rotate(180deg);
}

.header {
	position: relative;
	height: 110px;
	width: 100%;
	padding: 30px;
	box-sizing: border-box;
}

.header div, .header a {
	display: inline-block;
