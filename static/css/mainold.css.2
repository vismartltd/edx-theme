	border-radius: 25px;
	-webkit-transition: all 0.25s ease;
	-o-transition: all 0.25s ease;
	-moz-transition: all 0.25s ease;
	transition: all 0.25s ease;
}

.exit-img {
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-200%, -50%);
	height: 100%;
	width: 0;
	border: 1px;
	border-radius: 25px;
	opacity: 0.5;
	-webkit-transition: all 0.25s ease;
	-o-transition: all 0.25s ease;
	-moz-transition: all 0.25s ease;
	transition: all 0.25s ease;
}

.exit-img:hover {
