html, body {
	padding: 0;
	margin: 0;
}


@font-face {
	font-family: Circe;
	src: font-url("//static/themes/ninenone/css/fonts/Circe.otf");
}

/*@font-face {
	font-family: Circe-Extra-Bold;
	src: font-url("CirceExtraBold.otf");
}*/

@font-face {
	font-family: Circe-Bold;
	src: font-url("/static/themes/ninenone/css/fonts/CirceBold.otf");
}

@font-face {
	font-family: Circe-Light;
	src: font-url("/static/themes/ninenone/css/fonts/CirceLight.otf");
}

